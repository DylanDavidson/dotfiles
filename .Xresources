!!!!
!! Rofi
!!!!

! Keyboard Shortcuts
rofi.key-window: control+space
rofi.key-run: control+shift+r

! Colors
rofi.color-enabled: true
rofi.color-window: #504945, #504945, #504945
rofi.color-normal: #504945, #ebdbb2, #504945, #ebdbb2, #504945
rofi.color-active: #504945, #689d6a, #504945, #ebdbb2, #689d6a
rofi.color-urgent: #504945, #cc241d, #504945, #ebdbb2, #cc241d

!!!!
!! Terminal
!!!!

! General Settings
URxvt.scrollBar: false
URxvt.termName: xterm-256color

! Fonts
URxvt.font: xft:Source Code Pro Light:size=11:hinting=True;antialias:True

! Colors
URxvt.background: #282828
URxvt.foreground: #fbf1c7
! Black & Dark Grey
URxvt.color0: #282828
URxvt.color8: #3c3836
! Reds
URxvt.color1: #cc241d
URxvt.color9: #fb4934
! Greens
URxvt.color2: #98971a
URxvt.color10: #b8bb26
! Yellows
URxvt.color3: #d79921
URxvt.color11: #fabd2f
! Blues
URxvt.color4: #458588
URxvt.color12: #83a598
! Purples/Pink
URxvt.color5: #b16286
URxvt.color13: #d3869b
! Aqua/Teal/Cyan
URxvt.color6: #689d6a
URxvt.color14: #8ec07c
! White
URxvt.color7: #a89984
URxvt.color15: #ebdbb2
! Shift-Control-V to paste
URxvt.keysym.Shift-Control-V: perl:clipboard:paste
URxvt.iso14755: False
URxvt.perl-ext-common: default,clipboard,matcher,selection-to-clipboard
! Enables URL Links
URxvt.urlLauncher: /usr/bin/google-chrome-beta
URxvt.matcher.button: 1
URxvt.matcher.pattern.1: \\bwww\\.[\\w-]+\\.[\\w./?&@#-]*[\\w/-]
URxvt.matcher.pattern.2: \\bfile:\/\/.*
